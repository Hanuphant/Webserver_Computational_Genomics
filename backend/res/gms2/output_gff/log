grep -v '>' /home/groupa/final_assemblies/assemblies/CGT1225/contigs.fasta >> tmpseq.fna
/home/groupa/gene_prediction/gms2/gmhmmp2 -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s tmpseq.fna -o itr_0.lst --mgm_type auto 
/home/groupa/gene_prediction/gms2/biogem gms2-training -s tmpseq.fna -l itr_0.lst -m itr_1.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 0 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --run-motif-search false --genome-group D
/home/groupa/gene_prediction/gms2/gmhmmp2 -m itr_1.mod -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s tmpseq.fna -o itr_1.lst --format train
/home/groupa/gene_prediction/gms2/compp -n -a itr_0.lst -b itr_1.lst -G
/home/groupa/gene_prediction/gms2/biogem gms2-training -s tmpseq.fna -l itr_1.lst -m itr_2.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group A --ga-upstr-len-rbs 20 --align right --ga-width-rbs 6 --ga-upstr-len-prom 40 --ga-width-prom 12
/home/groupa/gene_prediction/gms2/gmhmmp2 -m itr_2.mod -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s tmpseq.fna -o itr_2.lst --format train
/home/groupa/gene_prediction/gms2/compp -n -a itr_1.lst -b itr_2.lst -G
/home/groupa/gene_prediction/gms2/biogem experiment promoter-is-valid-for-archaea --fnmod itr_2.mod --dist-thresh 14 --score-thresh 0.1 --window-size 1
mv itr_2.mod  groupA1.mod
mv itr_2.lst groupA1.lst
/home/groupa/gene_prediction/gms2/biogem gms2-training -s tmpseq.fna -l itr_1.lst -m itr_2.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group A2 --ga-upstr-len-rbs 20 --align right --ga-width-rbs 6 --ga-upstr-len-prom 40 --ga-width-prom 12 --ga-extended-sd TAAGGAGGTGA
/home/groupa/gene_prediction/gms2/gmhmmp2 -m itr_2.mod -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s tmpseq.fna -o itr_2.lst --format train
/home/groupa/gene_prediction/gms2/compp -n -a itr_1.lst -b itr_2.lst -G
/home/groupa/gene_prediction/gms2/biogem experiment promoter-is-valid-for-archaea --fnmod itr_2.mod --dist-thresh 14 --score-thresh 0.1 --window-size 1
mv itr_2.mod  groupA2.mod
mv itr_2.lst groupA2.lst
/home/groupa/gene_prediction/gms2/biogem gms2-training -s tmpseq.fna -l itr_1.lst -m itr_2.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group B --gb-upstr-len-rbs 20 --align right --gb-width-rbs 6 --gb-upstr-len-prom 20 --gb-width-prom 6 --gb-extended-sd TAAGGAGGTGA
/home/groupa/gene_prediction/gms2/gmhmmp2 -m itr_2.mod -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s tmpseq.fna -o itr_2.lst --format train
/home/groupa/gene_prediction/gms2/compp -n -a itr_1.lst -b itr_2.lst -G
/home/groupa/gene_prediction/gms2/biogem experiment promoter-is-valid-for-bacteria --fnmod itr_2.mod --dist-thresh 14 --score-thresh 0.25 --window-size 1 --min-leaderless-percent 11 --min-leaderless-count 100 --fnlabels itr_2.lst --fnseq tmpseq.fna
/home/groupa/gene_prediction/gms2/biogem experiment promoter-and-rbs-match --fnmod itr_2.mod --match-thresh 3
mv itr_2.mod  groupB.mod
mv itr_2.lst groupB.lst
/home/groupa/gene_prediction/gms2/biogem gms2-training -s tmpseq.fna -l itr_1.lst -m itr_2.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group C --gc-upstr-len-rbs 20 --align right --gc-width-rbs 6
/home/groupa/gene_prediction/gms2/gmhmmp2 -m itr_2.mod -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s tmpseq.fna -o itr_2.lst --format train
/home/groupa/gene_prediction/gms2/compp -n -a itr_1.lst -b itr_2.lst -G
/home/groupa/gene_prediction/gms2/biogem experiment rbs-consensus-and-16s-match --fnmod itr_2.mod --allow-ag-sub
mv itr_2.mod  groupC.mod
mv itr_2.lst groupC.lst
/home/groupa/gene_prediction/gms2/biogem gms2-training -s tmpseq.fna -l itr_1.lst -m itr_2.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group D --gd-upstr-len-rbs 20 --align right --gd-width-rbs 6
/home/groupa/gene_prediction/gms2/gmhmmp2 -m itr_2.mod -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s tmpseq.fna -o itr_2.lst --format train
/home/groupa/gene_prediction/gms2/compp -n -a itr_1.lst -b itr_2.lst -G
/home/groupa/gene_prediction/gms2/biogem experiment match-rbs-to-16s --fnlabels itr_2.lst --match-to TAAGGAGGTGA --min-match 4 --allow-ag-sub
/home/groupa/gene_prediction/gms2/biogem gms2-training -s tmpseq.fna -l itr_2.lst -m itr_3.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group D --gd-upstr-len-rbs 20 --align right --gd-width-rbs 6
/home/groupa/gene_prediction/gms2/gmhmmp2 -m itr_3.mod -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s tmpseq.fna -o itr_3.lst --format train
/home/groupa/gene_prediction/gms2/compp -n -a itr_2.lst -b itr_3.lst -G
/home/groupa/gene_prediction/gms2/biogem gms2-training -s tmpseq.fna -l itr_3.lst -m itr_4.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group D --gd-upstr-len-rbs 20 --align right --gd-width-rbs 6
/home/groupa/gene_prediction/gms2/gmhmmp2 -m itr_4.mod -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s tmpseq.fna -o itr_4.lst --format train
/home/groupa/gene_prediction/gms2/compp -n -a itr_3.lst -b itr_4.lst -G
cp itr_4.mod GMS2.mod
/home/groupa/gene_prediction/gms2/gmhmmp2 -m GMS2.mod -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s /home/groupa/final_assemblies/assemblies/CGT1225/contigs.fasta -o /home/groupa/gene_prediction/gms2/output_gff/CGT1225.gff --format gff 
