/home/groupa/gene_prediction/gms2/biogem gms2-training -s tmpseq.fna -l itr_1.lst -m itr_2.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group D --gd-upstr-len-rbs 20 --align right --gd-width-rbs 6
/home/groupa/gene_prediction/gms2/gmhmmp2 -m itr_2.mod -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s tmpseq.fna -o itr_2.lst --format train
/home/groupa/gene_prediction/gms2/compp -n -a itr_1.lst -b itr_2.lst -G
/home/groupa/gene_prediction/gms2/biogem experiment match-rbs-to-16s --fnlabels itr_2.lst --match-to TAAGGAGGTGA --min-match 4 --allow-ag-sub
