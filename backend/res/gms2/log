/home/groupa/gene_prediction/gms2/gmhmmp2 -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s tmpseq.fna -o itr_0.lst --mgm_type auto 
/home/groupa/gene_prediction/gms2/biogem gms2-training -s tmpseq.fna -l itr_0.lst -m itr_1.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 0 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --run-motif-search false --genome-group D
/home/groupa/gene_prediction/gms2/gmhmmp2 -m itr_1.mod -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s tmpseq.fna -o itr_1.lst --format train
/home/groupa/gene_prediction/gms2/compp -n -a itr_0.lst -b itr_1.lst -G
/home/groupa/gene_prediction/gms2/biogem gms2-training -s tmpseq.fna -l itr_1.lst -m itr_2.mod --order-coding 5 --order-noncoding 2 --only-train-on-native 1 --genetic-code 11 --order-start-context 2 --fgio-dist-thr 25 --genome-group A --ga-upstr-len-rbs 20 --align right --ga-width-rbs 6 --ga-upstr-len-prom 40 --ga-width-prom 12
/home/groupa/gene_prediction/gms2/gmhmmp2 -m itr_2.mod -M /home/groupa/gene_prediction/gms2/mgm_11.mod -s tmpseq.fna -o itr_2.lst --format train
/home/groupa/gene_prediction/gms2/compp -n -a itr_1.lst -b itr_2.lst -G
/home/groupa/gene_prediction/gms2/biogem experiment promoter-is-valid-for-archaea --fnmod itr_2.mod --dist-thresh 14 --score-thresh 0.1 --window-size 1
mv itr_2.mod  groupA1.mod
mv itr_2.lst groupA1.lst
